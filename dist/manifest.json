{
  "manifest_version": 3,
  "name": "Click downloads Links",
  "description": "To copy links by 20",
  "version": "0.1.0",
  "action": {
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "bookmarks",
    "webNavigation",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "commands": {
    "hot-link": {
      "suggested_key": {
        "default": "Alt+H",
        "mac": "Alt+U"
      },
      "description": "Put link on hot folder"
    },
    "awesome-link": {
      "suggested_key": {
        "default": "Alt+A",
        "mac": "Alt+J"
      },
      "description": "Put link on hot folder"
    },
    "ok-link": {
      "suggested_key": {
        "default": "Alt+O",
        "mac": "Alt+O"
      },
      "description": "Put link on hot folder"
    },
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+H",
        "mac": "Alt+H"
      }
    }
  }
}
